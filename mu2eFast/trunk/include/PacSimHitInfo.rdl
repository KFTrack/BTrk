#ifndef PacSimHitInfo_HH
#define PacSimHitInfo_HH

#include <Rtypes.h>
#define STRINGSIZE 200

struct PacSimHitInfo {
  Int_t shi, sheffect,shelemnum,shtypenum, shmeastype;  
  Float_t shgloblen, shpathlen, shradlen, shintlen, shradlenint, shintlenint;
  Float_t shx, shy, shz, shmomin, shmomout, shtime;
  
  virtual ~PacSimHitInfo() {}
  PacSimHitInfo() : shi(-1),sheffect(-1),shelemnum(-1),shtypenum(-1),shmeastype(-1),shgloblen(-1.),shpathlen(-1.),shradlen(-1.),shintlen(-1.),shradlenint(-1.),shintlenint(-1.),shx(0.),shy(0.),shz(0.),shmomin(0.),shmomout(0.),shtime(-1.)
  {}

  PacSimHitInfo(const PacSimHitInfo& other) : shi(other.shi),sheffect(other.sheffect),shelemnum(other.shelemnum),shtypenum(other.shtypenum),shmeastype(other.shmeastype),shgloblen(other.shgloblen),shpathlen(other.shpathlen),shradlen(other.shradlen),shintlen(other.shintlen),shradlenint(other.shradlenint),shintlenint(other.shintlenint),shx(other.shx),shy(other.shy),shz(other.shz),shmomin(other.shmomin),shmomout(other.shmomout),shtime(other.shtime)
  {}
  
  static const char* rootnames() {
    return "shi/I:sheffect/I:shelemnum/I:shtypenum/I:shmeastype/I:shgloblen/F:shpathlen/F:shradlen/F:shintlen/F:shradlenint/F:shintlenint/F:shx/F:shy/F:shz/F:shmomin/F:shmomout/F:shtime/F"; }
    
    ClassDef(PacSimHitInfo,1)
};

#endif
