// struct to compare trajectories

#ifndef TrajDiff_HH
#define TrajDiff_HH

#include <Rtypes.h>
#define STRINGSIZE 200

struct TrajDiff {
  Int_t shi,shj;
  Float_t startglen, endglen, ddiff, ddiff2;
  
  virtual ~TrajDiff() {}
  TrajDiff() : shi(-1),shj(-1),startglen(-1.0),endglen(-1.0),ddiff(-1.0),ddiff2(-1.0)
  {}

  TrajDiff(const TrajDiff& other) : shi(other.shi),startglen(other.startglen),endglen(other.endglen),ddiff(other.ddiff),ddiff2(other.ddiff2)
  {}
  
  static const char* rootnames() {
    return "shi/I:shj/I:sheffect/startglen/F:endglen/F:ddiff/F:ddiff2/F";}
    
    ClassDef(TrajDiff,1)
};

#endif
