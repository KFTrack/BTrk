import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper();

mainlib = helper.make_mainlib ( [ 'CLHEP',
                                ] )

helper.make_plugins( [ mainlib,
                     ] )

helper.make_dict_and_map( [ mainlib,
                          ] )

